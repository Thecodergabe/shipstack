{
  "name": "shipstack",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "generate:usps:auth": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/oauth2_3_update.yaml --output src/usps/auth/generated --client fetch",
    "generate:usps:rates": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/domestic-prices_10.yaml --output src/usps/rates/generated --client fetch",
    "generate:usps:address": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/addresses-v3r2_2.yaml --output src/usps/address/generated --client fetch",
    "generate:usps:labels": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/labels_update_1.yaml --output src/usps/labels/generated --client fetch",
    "generate:usps:intl-prices": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/international-prices_4.yaml --output src/usps/international/prices/generated --client fetch",
    "generate:usps:intl-labels": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/international-labels_8.yaml --output src/usps/international/labels/generated --client fetch",
    "generate:usps:tracking": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/tracking-v3r2_4.yaml --output src/usps/tracking/generated --client fetch",
    "generate:usps:appointments": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/appointments-all-mail-classes.yaml --output src/usps/appointments/generated --client fetch",
    "generate:usps:carrier-pickup": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/carrier-pickup_3.yaml --output src/usps/pickup/generated --client fetch",
    "generate:usps:adjustments": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/adjustments_0.yaml --output src/usps/adjustments/generated --client fetch",
    "generate:usps:disputes": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/disputes%20%282%29.yaml --output src/usps/disputes/generated --client fetch",
    "generate:usps:service-standards": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/service-standards_2_0.yaml --output src/usps/service-standards/generated --client fetch",
    "generate:usps:shipping-options": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/shipping-options_8.yaml --output src/usps/options/generated --client fetch",
    "generate:usps:subscriptions": "openapi --input https://developers.usps.com/sites/default/files/apidoc_specs/subscriptions-tracking-v3r2_1.yaml --output src/usps/subscriptions/generated --client fetch",

    "generate:usps": "npm run generate:usps:auth && npm run generate:usps:rates && npm run generate:usps:address && npm run generate:usps:labels && npm run generate:usps:intl-prices && npm run generate:usps:intl-labels && npm run generate:usps:tracking && npm run generate:usps:appointments && npm run generate:usps:carrier-pickup && npm run generate:usps:adjustments && npm run generate:usps:disputes && npm run generate:usps:service-standards && npm run generate:usps:shipping-options && npm run generate:usps:subscriptions",
    "generate:fedex:address": "openapi --input specs/fedex/address-validation.json --output src/fedex/address/generated --client fetch",
    "generate:fedex:availability": "openapi --input specs/fedex/service-availability.json --output src/fedex/availability/generated --client fetch",
    "generate:fedex:rates": "openapi --input specs/fedex/rate.json --output src/fedex/rates/generated --client fetch",
    "generate:fedex:ship": "openapi --input specs/fedex/ship.json --output src/fedex/ship/generated --client fetch",
    "generate:fedex": "npm run generate:fedex:address && npm run generate:fedex:availability && npm run generate:fedex:rates && npm run generate:fedex:ship",
    "generate:all": "npm run generate:usps && npm run generate:fedex"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "openapi-typescript-codegen": "^0.29.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3"
  },
  "dependencies": {
    "cross-fetch": "^4.1.0"
  }
}
